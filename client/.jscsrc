{
  "preset": "google",
  "validateIndentation": {
    "value": 2,
    "allExcept": ["comments"]
  },
  "maximumLineLength": {
    "value": 100,
    "allExcept": ["comments"]
  },
  "disallowMultipleVarDecl": false,
  "disallowMultipleLineBreaks": false,
  "disallowTrailingWhitespace": "ignoreEmptyLines",
  "requireCamelCaseOrUpperCaseIdentifiers": "ignoreProperties"
}
